[Unit]
Description=Memcache Service

[Service]
ExecStartPre=-/bin/docker kill memcache
ExecStartPre=-/bin/docker rm -v memcache
ExecStartPre=/bin/docker pull memcached
ExecStart=/usr/bin/bash -c "/bin/docker run --name=memcache -p 11211:11211 memcached"
ExecStop=/bin/docker stop memcache
ExecStopPost=/bin/docker rm -v memcache

[X-Fleet]
Conflicts=memcache.service
